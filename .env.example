# Environment Variables Template
# Copy this file to .env and fill in your actual values

# ServiceNow Configuration
VITE_SERVICENOW_INSTANCE=your-instance-name
VITE_SERVICENOW_CLIENT_ID=your-servicenow-client-id
VITE_SERVICENOW_CLIENT_SECRET=your-servicenow-client-secret
VITE_SERVICENOW_REDIRECT_URI=http://localhost:5173/callback

# Azure AD / Microsoft Graph Configuration
VITE_AZURE_CLIENT_ID=your-azure-client-id
VITE_AZURE_TENANT_ID=your-azure-tenant-id
VITE_AZURE_REDIRECT_URI=http://localhost:5173
